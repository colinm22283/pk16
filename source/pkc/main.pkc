const string str_a = "STRING A";
const string str_b = "STRING B";

extern def gpu_print_str(str, offset) requires(all);

def print_strings(str_pp, count) requires(a, b, c, d) {
    c <= str_pp;
    c <+ count;

    while str_pp < c {
        gpu_print_str(str_pp, 0);

        str_pp <+ 1;
    }
}

def main() requires(all) {
    string strs[2];

    a = strs;
    str_a __>^ a
    str_b __>  a

    print_strings(strs, 2);
}