def mult(x, y) {
    alias accum x;
    alias count y;
    alias reserve c; c = x;

    d <= 0;
    while count > d {
        accum + reserve;

        forget d; d <= -1;
        count + d;
    }

    return <= accum;
}

def memcpy(dst, src, count) {
    alias end d; end = dst;
    end <+ count;

    while dst < end {
        c ^<_ src;
        c _>^ dst;
    }
}